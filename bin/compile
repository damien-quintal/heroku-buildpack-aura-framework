#!/usr/bin/env bash
# bin/compile <build-dir> <cache-dir> <env-dir>

set -o errexit
set -o pipefail
set -o nounset
unset GIT_DIR

function message() {
  echo "-----> $*..."
}

BUILD_DIR=${1:-}
CACHE_DIR=${2:-}
ENV_DIR=${3:-}

echo $BUILD_DIR
echo $CACHE_DIR
echo $ENV_DIR

message "Start jetty server"
./bin/jetty-start
message "Aura Framework running!"